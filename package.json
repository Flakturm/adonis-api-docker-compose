{
  "name": "adonis-api-app",
  "version": "4.1.0",
  "adonis-version": "4.1.0",
  "description": "Adonisjs boilerplate for API server with pre-configured JWT and docker",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev-kue": "nodemon --watch app --watch bootstrap --watch config --watch resources --watch .env --watch libs -x \"node ace kue:listen\"",
    "kue": "node ace kue:listen",
    "dev-scheduler": "nodemon --watch app --watch bootstrap --watch config --watch resources --watch .env --watch libs -x \"node ace run:scheduler\"",
    "scheduler": "node ace run:scheduler",
    "test": "node ace test"
  },
  "keywords": [
    "adonisjs",
    "adonis-app"
  ],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/antl": "^2.0.7",
    "@adonisjs/auth": "^3.0.7",
    "@adonisjs/bodyparser": "^2.0.5",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.9",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/validator": "^5.0.6",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/redis": "^2.0.7",
    "adonis-kue": "^5.0.1",
    "adonis-scheduler": "^3.0.2",
    "adonis-swagger": "^1.2.7",
    "pg": "^7.4.3"
  },
  "devDependencies": {},
  "autoload": {
    "App": "./app"
  }
}
